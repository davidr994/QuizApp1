<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="250.0" prefWidth="150.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="menu.Controller">
   <children>
      <VBox layoutX="25.0" layoutY="25.0" prefHeight="200.0" prefWidth="100.0">
         <children>
            <Label alignment="CENTER" prefHeight="17.0" prefWidth="101.0" text="QUIZ APP" />
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="25.0">
               <children>
                  <Button alignment="CENTER" maxWidth="90.0" mnemonicParsing="false" onAction="#startButton" prefHeight="40.0" text="start" />
                  <Button alignment="CENTER" maxWidth="90.0" mnemonicParsing="false" onAction="#quitButton" prefHeight="40.0" text="quit" />
                  <Button alignment="CENTER" maxWidth="90.0" mnemonicParsing="false" onAction="#scoreButton" prefHeight="40.0" text="score" />
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
